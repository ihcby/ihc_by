= render partial: 'entity_breadcrumb'
.row
  .col-lg-6.col-lg-offset-3
    .well
      = simple_form_for([@account, @laboratory, @trial_type]) do |f|
        %legend
          = form_title(@trial_type)

        .form-inputs
          = f.input :name

        .form-actions
          = f.button :submit, t('common.actions.save')

